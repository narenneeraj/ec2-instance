pipeline {
agent any
  stages {
  
    stage('test'){
      steps {
      withCredentials([[
    $class: 'AmazonWebServicesCredentialsBinding',
    credentialsId: "aws_creds",
    accessKeyVariable: 'AWS_ACCESS_KEY_ID',
    secretKeyVariable: 'AWS_SECRET_ACCESS_KEY'
]]) {
      sh '''
      whoami
      aws configure
      sudo terraform init
      sudo terraform apply --auto-approve
      '''
}
      }
      }
    }

}
